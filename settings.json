{
  /* Workbench Style */
  "workbench.colorTheme": "Omni",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "workbench.sideBar.location": "right",
  "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": true,
  "workbench.editor.enablePreview": false,
  "workbench.layoutControl.enabled": false,
  "workbench.tree.enableStickyScroll": false,
  /* Window Style */
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "native",
  "window.commandCenter": false,
  /* Editor Style */
  "editor.lineNumbers": "on",
  "editor.wordWrap": "on",
  "editor.cursorStyle": "block",
  "editor.renderLineHighlight": "all",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.guides.bracketPairs": "active",
  "editor.tabSize": 2,
  "editor.rulers": [80, 120],
  "editor.bracketPairColorization.enabled": true,
  "editor.detectIndentation": true,
  "editor.linkedEditing": true,
  "editor.insertSpaces": false,
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": false,
  "editor.semanticHighlighting.enabled": false,
  /*
   *  FONTES
   *  CaskaydiaCove Nerd Font
   *  FantasqueSansM Nerd Font
   *  FiraCode Nerd Font
   *  Hack Nerd Font
   *  Hasklug Nerd Font
   *  Iosevka Nerd Font
   *  IosevkaTerm Nerd Font
   *  JetBrainsMono Nerd Font
   *  Lilex Nerd Font
   *  Monoid Nerd Font
   *  VictorMono Nerd Font
   *  === == => >= <= -> <- ~~ ~- != !==
   */
  "editor.fontFamily": "JetBrainsMono Nerd Font",
  "editor.fontWeight": "bold",
  "editor.fontSize": 14,
  "editor.lineHeight": 1.6,
  "editor.fontLigatures": true,
  /* Explorer Style */
  "explorer.compactFolders": false,
  /* Extensions */
  "extensions.ignoreRecommendations": true,
  /* Terminal Default */
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.fontFamily": "FantasqueSansM Nerd Font",
  /* Git */
  "git.suggestSmartCommit": false,
  /* Breadcrumbs */
  "breadcrumbs.enabled": false,
  /* Files Style */
  "files.insertFinalNewline": true,
  /* Code Formatter */
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer",
    "editor.formatOnSave": false
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  /* Inline Parameters */
  "inline-parameters.enabled": true,
  /* Tabnine AI Autocomplete for Javascript, Python, Typescript, PHP, Go, Java, Ruby & more */
  "tabnine.experimentalAutoImports": true,
  /* GitHub Pull Request */
  "githubPullRequests.createOnPublishBranch": "never",
  /* php cs fixer */
  // "php-cs-fixer.executablePathWindows": "${workspaceFolder}/tools/php-cs-fixer/vendor/bin/php-cs-fixer.bat",
  // "php-cs-fixer.executablePath": "${workspaceFolder}/tools/php-cs-fixer/vendor/bin/php-cs-fixer",
  "php-cs-fixer.executablePath": "php-cs-fixer.bat",
  "php-cs-fixer.onsave": false,
  "php-cs-fixer.formatHtml": false,
  /* Material-icon-theme */
  "material-icon-theme.folders.associations": {
    "adapters": "contract",
    "grpc": "pipe",
    "kube": "kubernetes",
    "main": "lib",
    "websockets": "pipe",
    "implementations": "core",
    "protos": "pipe",
    "entities": "class",
    "kafka": "pipe",
    "use-cases": "functions",
    "migrations": "tools",
    "schemas": "class",
    "useCases": "functions",
    "eslint-config": "tools",
    "typeorm": "database",
    "_shared": "shared",
    "mappers": "meta",
    "fakes": "mock",
    "modules": "components",
    "subscribers": "messages",
    "domain": "class",
    "protocols": "contract",
    "infra": "app",
    "view-models": "views",
    "presentation": "template",
    "dtos": "typescript",
    "http": "container",
    "providers": "include",
    "factories": "class",
    "repositories": "mappings",
    "bootstrap": "rules"
  },
  "material-icon-theme.files.associations": {
    ".env": "settings",
    ".env.example": "settings",
    ".php-cs-fixer.cache": "php-cs-fixer"
  },
  /* Code Spell Checker */
  "cSpell.language": "en,pt_PT,pt_BR, lorem",
  "cSpell.userWords": [
    "Caskaydia",
    "dtos",
    "esbenp",
    "Fantasque",
    "Hasklug",
    "Iosevka",
    "junstyle",
    "kube",
    "kubernetes",
    "Lilex",
    "Monoid",
    "Omni",
    "onsave",
    "stylesheet",
    "tabnine",
    "typeorm",
    "websockets"
  ],
  /* Apc Customize UI++ */
  "apc.electron": {
    "titleBarStyle": "hidden"
  },
  "apc.font.family": "JetBrainsMono Nerd Font",
  "apc.stylesheet": {
    ".title-label > h2": "display: none",
    ".editor-actions": "display: none"
  }
}
